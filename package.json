{
  "name": "GSuite OAuth MCP Server",
  "version": "0.1.0",
  "description": "Google OAuth MCP Server for Suite integration",
  "private": true,
  "type": "module",
  "bin": {
    "GSuite OAuth MCP Server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node --eval \"import('fs').then(fs => { fs.chmodSync('build/index.js', '755'); fs.chmodSync('build/scripts/setup-google-env.js', '755'); })\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js",
    "setup": "node build/scripts/setup-google-env.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "googleapis": "^129.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
